@model IEnumerable<ProjetFinal.ViewModels.PanierCommande>

<table class="table" style="margin-bottom:20px;">
    <tr>
        <th>@Html.DisplayNameFor(model => model.DateCreated)</th>
        <th>@Html.DisplayNameFor(model => model.DateFinie)</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.DateCreated)</td>
        <td>
            @Html.DisplayFor(modelItem => item.DateFinie) 
        </td>
        <td>
            @Html.ActionLink("Détail","Details",new { id = item.PanierId },new { name="commandeDetail"}) 
        </td>
    </tr>

}

</table>
